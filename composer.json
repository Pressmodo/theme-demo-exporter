{
	"name"        : "pressmodo/theme-demo-exporter",
	"description" : "",
	"keywords"    : [ "wordpress", "pressmodo", "sematico", "wp-cli", "comments", "generator" ],
	"homepage"    : "https://sematico.com",
	"license"     : "GPL-3.0-or-later",
	"authors"     : [
		{
			"name"     : "Alessandro Tesoro",
			"email"    : "hello@sematico.com",
			"homepage" : "https://sematico.com",
			"role"     : "Founder & MD"
		}
	],
	"support"     : {
		"issues" : "https://github.com/Pressmodo/theme-demo-exporter"
	},
	"autoload"    : {
		"psr-4" : {
			"Pressmodo\\CLI\\" : "src"
		},
		"files": [ "command.php" ]
	},
	"autoload-dev": {
        "psr-4": {
            "Pressmodo\\CLI\\": "tests"
        }
    },
	"require": {
		"php": ">=7.2",
		"composer/installers": "^1.9",
		"nelexa/zip": "^3.3",
		"spatie/db-dumper": "^2.18",
		"symfony/filesystem": "^5.1"
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
		"wp-coding-standards/wpcs": "dev-master"
	},
	"config": {
		"bin-dir": "bin",
		"sort-packages": true,
		"preferred-install": {
            "*": "dist"
        }
	},
	"scripts": {
        "test": "bin/phpunit"
    },
	"minimum-stability": "dev",
	"prefer-stable": true
}
